callout printf;
void main(){
	int i;
	// testing break and continue in for loop
	for (i=0, 5) {
		if (i == 2) {
			continue;
		}
		if (i == 4) {
			break;
		}
		printf("i=%d\n", i);
		continue;
	}

	// testing break and continue in while loop
	for (i=0, 3) {
		int loop;
		loop = 0;
		while (true) {
			loop += 1;
			if (loop < 3) {
				printf("hi%d", loop);
				continue;
			}
			if (loop >= 5) {
				break;
			}
			printf("la%d", loop);
		}
		printf("\n");
	}
}
